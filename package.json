{
  "name": "box-with-3-tabs",
  "version": "1.0.0",
  "description": "Simple Webpack-based project presenting three functionalities: 1) loading and parsing a localy stored file, 2) reading and parsing three rss feeds, 3) usage of Flickr API",
  "main": "dist/app.min.js",
  "author": {
    "name": "Konrad Osiecki",
    "email": "konrad.osiecki@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run-script clean && npm run-script template && npm run-script systemjs && npm run-script vendor && npm run-script app",
    "app": "webpack --color --config ./config/webpack.config.js --mode=build ",
    "vendor": "webpack --color --config ./config/webpack.config.vendor.js",
    "dev": "npm run-script template && npm run-script systemjs && npm run-script vendor && npm run-script serve-app",
    "clean": "cleandir ./dist",
    "template": "copyfiles -f ./src/tpl/index.html dist",
    "systemjs": "copyfiles -f ./node_modules/systemjs/dist/system.js dist/js",
    "test": "karma start ./config/karma.conf.js --single-run",
    "cover": "open coverage/lcov-report/index.html",
    "eslint": "eslint src --ext .js",
    "eslint:fix": "eslint src .js --fix",
    "eslint:test": "eslint test --ext .js",
    "serve-app": "webpack-dev-server --progress --color --watch --config ./config/webpack.config.js",
    "serve-proxy": "webpack-dev-server --progress --color --watch --config ./config/webpack.config.proxy.js"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "clean-dir": "^1.0.3",
    "copyfiles": "^1.2.0",
    "css-loader": "0.9.1",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.7.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "imports-loader": "0.6.3",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jquery": "^3.2.0",
    "json-loader": "0.5.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "less": "2.5.1",
    "less-loader": "2.0.0",
    "load-grunt-configs": "0.4.1",
    "load-grunt-tasks": "2.0.0",
    "lodash": "4.17.4",
    "mocha": "3.1.2",
    "moment": "^2.18.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "0.8.3",
    "systemjs": "^0.19.47",
    "url-parse": "^1.1.8",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "^0.14.1",
    "yargs": "6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gundik/box-with-three-tabs"
  },
  "private": true,
  "files": [
    "dist",
    "LICENSE"
  ]
}
